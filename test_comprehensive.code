def __main__() : {
    // Numeric operations
    int a = 10;
    int b = 20;
    int c = a + b;
    float d = 3.14;
    float e = a + d;  // int + float -> float
    
    // Boolean operations
    bool f = True;
    bool g = False;
    bool h = f and g;
    bool i = f or g;
    bool j = not f;
    
    // Comparison operations
    bool k = a > b;
    bool l = d <= e;
    bool m = a == b;
    bool n = d != e;
    
    // Mixed type comparisons
    bool o = a > d;  // int > float is valid
    
    // Invalid operations that should be caught:
    // bool p = a and b;  // Error: int operands for and
    // bool q = f + g;    // Error: bool operands for +
    // bool r = a == d;   // Error: different types for ==
}
